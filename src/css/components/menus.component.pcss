#accessabar .menu {
    height: 10rem;
    width: 10rem;
    background: var(--near-white);
    border: 4px solid var(--dark-grey);
    z-index: 999;

    .menu-header {
        padding: 0.5em;
        background: var(--dark-grey);
        font-size: 1rem;
        color: var(--near-white);
        font-family: var(--header-font);

        &:hover {
            cursor: move;
        }
    }
}
