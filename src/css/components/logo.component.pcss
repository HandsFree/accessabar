#accessabar .ab-logo, #accessabar .ab-logo-large {
    display: flex;
    justify-content: center;
    align-items: center;
}

#accessabar .ab-logo {
    padding: 0.5em 1em;
    width: 10em;
}

#accessabar .ab-logo-large {
    padding: 2em;
    width: 20em;
    height: 10em;
}

#accessabar .ab-logo-img {
    width: 100%;
    height: 100%;
    background-image: url("../images/accessabar-word.png");
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: center;
}
