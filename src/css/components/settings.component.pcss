#accessabar .ab-settings {
    display: grid;
    grid-template-rows: 10rem 100%;
    grid-template-areas: "header" "page";
    position: absolute;
    top: 0;
    left: 0;
    height: 100vh;
    width: 100vw;
    max-width: 100vw;
    z-index: 2140000015;
    background: var(--near-white);
}

#accessabar .ab-settings-logo {
    grid-area: header;
}
