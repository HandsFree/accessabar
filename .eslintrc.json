{
  "extends": "./node_modules/gts/",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "node/no-unpublished-require": "off",
    "node/no-unsupported-features/es-syntax": ["error", {
      "version": ">=14.x",
      "ignores": [
        "modules"
      ]
    }]
  }
}
