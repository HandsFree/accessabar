{
  "name": "@handsfree/accessabar",
  "version": "0.11.4",
  "description": "Accessabar is a toolbar that provides a range of features to make any website accessible.",
  "author": "Elliott Judd <elliott.judd@hands-free.co.uk>",
  "license": "AGPL-3.0-or-later",
  "main": "public/dist/accessabar/accessabar.bundle.js",
  "scripts": {
    "prepare": "rimraf \"./public/dist/accessabar/*\" \"./node_modules/.cache/*\" && webpack --env.mode=\"production\" --env.npm=\"enabled\"",
    "build": "webpack --env.mode=\"production\"",
    "b": "webpack --env.mode=\"development\"",
    "b:v": "webpack --env.mode=\"development\" --env.verbose=\"enabled\"",
    "server": "webpack-dashboard -- webpack-dev-server --env.mode=\"development\" --env.server=\"enabled\" --env.dash=\"enabled\"",
    "s": "webpack-dashboard -- webpack-dev-server --env.mode=\"development\" --env.server=\"enabled\" --env.dash=\"enabled\"",
    "bp": "webpack --env.mode=\"production\"",
    "bp:v": "webpack --env.mode=\"production\" --env.verbose=\"enabled\"",
    "clean": "rimraf \"./public/dist/accessabar/*\" \"./node_modules/.cache/*\"",
    "cypress": "cypress open",
    "cypress:run-all": "cypress run",
    "ava": "ava",
    "test": "ava && cypress run",
    "serve": "serve ./public/",
    "now-build": "yarn bp",
    "check": "gts check src/**/*.ts",
    "fix": "gts fix src/**/*.ts"
  },
  "devDependencies": {
    "@babel/core": "7.12.0",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.0",
    "@babel/preset-env": "7.12.0",
    "@babel/preset-typescript": "7.12.0",
    "@types/node": "13.11.1",
    "@types/webpack": "4.41.22",
    "@types/webpack-env": "1.15.3",
    "autoprefixer": "9.8.6",
    "ava": "3.13.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "browser-sync": "2.26.12",
    "browser-sync-webpack-plugin": "2.2.2",
    "cache-loader": "4.1.0",
    "css-hot-loader": "1.4.4",
    "css-loader": "5.0.0",
    "cypress": "5.4.0",
    "eslint-plugin-cypress": "2.11.2",
    "file-loader": "6.1.1",
    "gts": "3.0.1",
    "hard-source-webpack-plugin": "0.13.1",
    "json5-loader": "4.0.1",
    "mini-css-extract-plugin": "1.0.0",
    "offline-plugin": "5.0.7",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss-custom-media": "7.0.8",
    "postcss-easy-import": "3.0.0",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "4.0.4",
    "postcss-nested": "4.2.3",
    "postcss-nested-ancestors": "2.0.0",
    "postcss-nested-props": "2.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-sorting": "5.0.1",
    "resource-hints-webpack-plugin": "0.0.2",
    "rimraf": "3.0.2",
    "rucksack-css": "1.0.2",
    "source-map-loader": "1.1.1",
    "stylelint": "13.7.2",
    "stylelint-config-recommended": "3.0.0",
    "terser-webpack-plugin": "4.2.3",
    "thread-loader": "3.0.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dashboard": "3.2.1",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "7.12.0",
    "@simonwep/pickr": "1.7.4",
    "@webcomponents/custom-elements": "1.4.2",
    "bignumber.js": "9.0.1",
    "hotkeys-js": "^3.8.1",
    "hyperapp": "2.0.4",
    "iso-639-1": "2.1.4",
    "normalize.css": "8.0.1",
    "serve": "11.3.2",
    "tippy.js": "6.2.7",
    "uuid": "8.3.1"
  }
}
