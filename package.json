{
  "name": "@handsfree/accessabar",
  "version": "0.8.9",
  "description": "Accessabar is a toolbar that provides a range of features to make any website accessible.",
  "author": "Elliott Judd <elliott.judd@hands-free.co.uk>",
  "license": "AGPL-3.0-or-later",
  "main": "public/dist/accessabar/accessabar.bundle.js",
  "scripts": {
    "prepare": "rimraf './public/dist/accessabar/*' './node_modules/.cache/*' && webpack --env.mode='production' --env.npm='enabled'",
    "build": "webpack --env.mode='production'",
    "b": "webpack --env.mode='development'",
    "b:v": "webpack --env.mode='development' --env.verbose='enabled'",
    "server": "webpack-dashboard -- webpack-dev-server --env.mode='development' --env.server='enabled' --env.dash='enabled'",
    "s": "webpack-dashboard -- webpack-dev-server --env.mode='development' --env.server='enabled' --env.dash='enabled'",
    "bp": "webpack --env.mode='production'",
    "bp:v": "webpack --env.mode='production' --env.verbose='enabled'",
    "lint": "tslint",
    "lint-all": "tslint 'src/**/*.ts'",
    "clean": "rimraf './public/dist/accessabar/*' './node_modules/.cache/*'",
    "cypress": "cypress open",
    "cypress:run-all": "cypress run",
    "ava": "ava",
    "test": "ava && cypress run",
    "serve": "serve ./public/",
    "now-build": "yarn bp"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/jquery": "3.3.31",
    "@types/node": "10.14.10",
    "@types/webpack": "4.39.1",
    "@types/webpack-env": "1.14.0",
    "autoprefixer": "9.6.1",
    "ava": "2.3.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "browser-sync": "2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "cache-loader": "4.1.0",
    "css-hot-loader": "1.4.4",
    "css-loader": "3.2.0",
    "cypress": "3.4.1",
    "eslint": "6.2.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "file-loader": "4.2.0",
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "hard-source-webpack-plugin": "0.13.1",
    "json5-loader": "3.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "offline-plugin": "5.0.7",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-custom-media": "7.0.8",
    "postcss-easy-import": "3.0.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.2",
    "postcss-nested-ancestors": "2.0.0",
    "postcss-nested-props": "2.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-sorting": "5.0.1",
    "resource-hints-webpack-plugin": "0.0.2",
    "rimraf": "3.0.0",
    "rucksack-css": "1.0.2",
    "sass-loader": "7.3.1",
    "source-map-loader": "0.2.4",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "2.2.0",
    "terser-webpack-plugin": "1.4.1",
    "thread-loader": "2.1.3",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.5.3",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.7",
    "webpack-dashboard": "3.1.0",
    "webpack-dev-server": "3.8.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "7.5.5",
    "@hyperapp/html": "1.1.1",
    "@simonwep/pickr": "1.4.0",
    "@webcomponents/custom-elements": "1.2.4",
    "bignumber.js": "9.0.0",
    "hyperapp": "2.0.1",
    "normalize.css": "8.0.1",
    "rxjs": "6.4.0",
    "serve": "11.0.0",
    "tippy.js": "3.4.1"
  }
}
