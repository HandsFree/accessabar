{
  "name": "accessabar",
  "version": "0.0.1",
  "description": "Accessabar",
  "author": "Elliott Judd <elliott.judd@hands-free.co.uk>",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "yarn bp",
    "b": "parallel-webpack -- --env.mode='development'",
    "b:v": "parallel-webpack -- --env.mode='development' --env.verbose='enabled'",
    "b:w": "webpack-dashboard -- parallel-webpack --watch -- --env.mode='development' --env.dash='enabled'",
    "b:wv": "webpack-dashboard -- parallel-webpack --watch -- --env.mode='development' --env.dash='enabled' --env.verbose='enabled'",
    "server": "yarn s",
    "s": "webpack-dashboard -- webpack-dev-server --env.mode='development' --env.server='enabled' --env.dash='enabled'",
    "bp": "parallel-webpack -- --env.mode='production'",
    "bp:v": "parallel-webpack -- --env.mode='production' --env.verbose='enabled'",
    "bp:w": "webpack-dashboard -- parallel-webpack --watch -- --env.mode='production' --env.dash='enabled'",
    "bp:wv": "webpack-dashboard -- parallel-webpack --watch -- --env.mode='production' --env.dash='enabled' --env.verbose='enabled'",
    "lint": "tslint",
    "lint-all": "tslint 'src/**/*.ts'",
    "clean": "rimraf './public/dist/accessabar/*' './public/views/*'  './node_modules/.cache/*'",
    "cypress": "cypress open",
    "cypress:run-all": "cypress run",
    "ava": "ava",
    "test": "yarn ava && yarn cypress:run-all"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-decorators": "7.2.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-typescript": "7.1.0",
    "@types/jquery": "3.3.28",
    "@types/node": "10.12.18",
    "@types/webpack": "4.4.21",
    "@types/webpack-env": "1.13.6",
    "autoprefixer": "9.4.3",
    "ava": "1.0.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "browser-sync": "2.26.3",
    "browser-sync-webpack-plugin": "2.2.2",
    "cache-loader": "1.2.5",
    "css-hot-loader": "1.4.3",
    "css-loader": "1.0.1",
    "cypress": "3.1.3",
    "eslint": "5.10.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "hard-source-webpack-plugin": "0.13.1",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "json5-loader": "1.0.1",
    "mini-css-extract-plugin": "0.5.0",
    "offline-plugin": "5.0.6",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "parallel-webpack": "2.3.0",
    "postcss-custom-media": "7.0.7",
    "postcss-easy-import": "3.0.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.1",
    "postcss-nested-ancestors": "2.0.0",
    "postcss-nested-props": "2.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-sorting": "4.0.1",
    "resource-hints-webpack-plugin": "0.0.2",
    "rimraf": "2.6.2",
    "rucksack-css": "1.0.2",
    "sass-loader": "7.1.0",
    "source-map-loader": "0.2.4",
    "stylelint": "9.9.0",
    "stylelint-config-recommended": "2.1.0",
    "thread-loader": "1.2.0",
    "ts-node": "7.0.1",
    "tslint": "5.12.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.2.2",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.28.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dashboard": "2.0.0",
    "webpack-dev-server": "3.1.11"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "7.2.0",
    "@hyperapp/html": "1.1.1",
    "@webcomponents/custom-elements": "1.2.1",
    "hyperapp": "1.2.9",
    "normalize.css": "8.0.1",
    "pickr-widget": "0.3.2",
    "tippy.js": "3.3.0"
  }
}
